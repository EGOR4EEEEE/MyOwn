def find_max(a, b, c):
    max_num = max(a, b, c)
    return max_num

num1 = int(input("Введите первое число: "))
num2 = int(input("Введите второе число: "))
num3 = int(input("Введите третье число: "))

max_number = find_max(num1, num2, num3)
print(f"Максимальное число из {num1}, {num2}, {num3} - это {max_number}")
